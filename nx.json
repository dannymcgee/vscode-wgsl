{
	"npmScope": "wgsl",
	"affected": {
		"defaultBase": "main"
	},
	"implicitDependencies": {
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		".eslintrc.json": "*"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "@nrwl/workspace/tasks-runners/default",
			"options": {
				"cacheableOperations": ["build", "lint", "test", "e2e"]
			}
		}
	},
	"targetDependencies": {
		"build": [
			{
				"target": "build",
				"projects": "dependencies"
			}
		]
	},
	"projects": {
		"client": {
			"tags": [],
			"implicitDependencies": ["grammar", "server"]
		},
		"grammar": {
			"tags": []
		},
		"parser": {
			"tags": []
		},
		"server": {
			"tags": []
		},
		"wgsl_plus": {
			"tags": []
		}
	},
	"workspaceLayout": {
		"appsDir": "packages",
		"libsDir": "packages"
	},
	"plugins": ["@nxrs/cargo"]
}
